<div class="jumbo show-listing">
  <h2 class="title">{{model.title}}</h2>
  <div class="right detail-section">
    <div class="detail owner">
      <strong>Director:</strong> {{model.owner}}
    </div>
    <div class="detail">
      <strong>Type:</strong> {{model.category}}
    </div>
    <div class="detail">
      <strong>Year:</strong> {{model.city}}
    </div>
    <div class="detail">
      <strong>Ratings:</strong> {{model.bedrooms}}
    </div>
    <p>&nbsp;</p>
    <p class="description">{{model.description}}</p>
  </div>
  <img src="{{model.image}}" class="rental-pic">
</div>
